    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_membersList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="0dp"
        app:layoutManager="LinearLayoutManager"
        />

</RelativeLayout>
